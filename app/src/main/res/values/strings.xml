    <style name="Ripple">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">false</item>
        <item name="android:focusable">true</item>
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>


    <style name="tvTitleStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:textSize">@dimen/text_title_block</item>
        <item name="android:textColor">@color/browser_actions_bg_grey</item>
    </style>


    <style name="rowSwitchStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textSize">@dimen/text_size_title</item>
    </style>

    <style name="rowdetails">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/screen_padding_2</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="android:maxLines">2</item>


    </style>

    <style name="rowAlonetvt">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/text_color</item>
        <item name="android:textSize">@dimen/text_size_title</item>

    </style>

    <style name="imageStyle">
        <item name="android:layout_width">@dimen/row_height</item>
        <item name="android:layout_height">@dimen/row_height</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/item_ic_padding</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">4dp</item>

    </style>

    <style name="swStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="colorControlActivated">@color/mbridge_cpb_red</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_marginRight">10dp</item>

    </style>

    <style name="MySwitch" parent="Theme.AppCompat.Light">
        <!-- активное состояние (30% прозрачности для фона) -->
        <item name="colorControlActivated">@android:color/holo_green_light</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <!-- неактивное состояние ползунка -->
        <item name="colorSwitchThumbNormal">@android:color/holo_orange_dark</item>

        <!-- цвет дорожки для неактивного состояния (30% прозрачности) -->
        <item name="android:colorForeground">@color/colorAccent</item>
    </style>

